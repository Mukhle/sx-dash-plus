{
	"manifest_version": 2,

	"name": "__MSG_extName__",
	"description": "__MSG_extDesc__",
	"default_locale": "da",
	"version": "0.4.5",
	"icons": {
		"64": "icon128.png",
		"128": "icon128.png"
	},
	"permissions": [
		"*://stavox.dk/dash/*"
	],
	"content_scripts": [
		{
			"js": ["staffcheck.js"],
			"matches": ["https://stavox.dk/dash/*"]
		},
		{
			"js": ["lookup.js"],
			"matches": ["https://stavox.dk/dash/lookup?lookupid=*"]
		},
		{
			"js": ["me.js"],
			"matches": ["https://stavox.dk/dash/me*"]
		},
		{
			"js": ["stafflist.js"],
			"matches": ["https://stavox.dk/dash/stafflist"]
		},
		{
			"js": ["rules.js"],
			"matches": ["https://stavox.dk/dash/rules"]
		},
		{
			"js": ["bans.js"],
			"matches": ["https://stavox.dk/dash/bans"]
		},
		{
			"js": ["latestbans.js"],
			"matches": ["https://stavox.dk/dash/latestbans"]
		},
		{
			"js": ["latestunbans.js"],
			"matches": ["https://stavox.dk/dash/latestunbans"]
		},
		{
			"js": ["lookupsearch.js"],
			"matches": ["https://stavox.dk/dash/*"],
			"exclude_matches": [
				"https://stavox.dk/dash/lookup*",
				"https://stavox.dk/dash/bans"
			]
		}
	]
}
