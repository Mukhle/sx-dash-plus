{
	"manifest_version": 2,

	"name": "__MSG_extName__",
	"description": "__MSG_extDesc__",
	"default_locale": "da",
	"version": "1.2.6",
	"icons": {
		"64": "icon128.png",
		"128": "icon128.png"
	},
	"permissions": ["*://stavox.dk/dash/*"],
	"content_scripts": [
		{
			"js": ["staffcheck.js"],
			"matches": ["*://stavox.dk/dash/*"]
		},
		{
			"js": ["fixes/tables.js"],
			"matches": ["*://stavox.dk/dash/*"]
		},
		{
			"js": ["fixes/stafflist.js"],
			"matches": ["*://stavox.dk/dash/stafflist*"]
		},
		{
			"js": ["fixes/traficlaws.js"],
			"matches": ["*://stavox.dk/dash/ptraficlaw*"]
		},
		{
			"js": ["steamids.js"],
			"css": ["steamids.css"],
			"matches": ["*://stavox.dk/dash/*"]
		},
		{
			"js": [
				"lookup/shared.js",
				"lookup/info.js",
				"lookup/notes.js",
				"lookup/punishments.js",
				"lookup/watcher.js"
			],
			"matches": ["*://stavox.dk/dash/lookup*?lookupid=*"]
		},
		{
			"js": ["lookup/shared.js", "lookup/punishments.js"],
			"matches": ["*://stavox.dk/dash/me*"]
		},
		{
			"js": ["rules.js"],
			"matches": ["*://stavox.dk/dash/rules*"]
		},
		{
			"js": ["lookupsearch.js"],
			"matches": ["*://stavox.dk/dash/*"],
			"exclude_matches": [
				"*://stavox.dk/dash/lookup*",
				"*://stavox.dk/dash/bans*"
			]
		}
	]
}
