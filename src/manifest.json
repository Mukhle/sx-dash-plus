{
	"manifest_version": 2,

	"name": "__MSG_extName__",
	"description": "__MSG_extDesc__",
	"default_locale": "da",
	"version": "1.1.1",
	"icons": {
		"64": "icon128.png",
		"128": "icon128.png"
	},
	"permissions": ["*://stavox.dk/dash/*"],
	"content_scripts": [
		{
			"js": ["staffcheck.js"],
			"matches": ["*://stavox.dk/dash/*"]
		},
		{
			"js": ["steamids.js"],
			"css": ["steamids.css"],
			"matches": ["*://stavox.dk/dash/*"]
		},
		{
			"js": ["tablechanges.js"],
			"css": ["tablechanges.css"],
			"matches": ["*://stavox.dk/dash/*"]
		},
		{
			"js": [
				"lookup_shared.js",
				"lookup_info.js",
				"lookup_notes.js",
				"lookup_punishments.js"
			],
			"matches": ["*://stavox.dk/dash/lookup*?lookupid=*"]
		},
		{
			"js": ["lookup_shared.js", "lookup_punishments.js"],
			"matches": ["*://stavox.dk/dash/me*"]
		},
		{
			"js": ["rules.js"],
			"matches": ["*://stavox.dk/dash/rules*"]
		},
		{
			"js": ["lookupsearch.js"],
			"matches": ["*://stavox.dk/dash/*"],
			"exclude_matches": [
				"*://stavox.dk/dash/lookup*",
				"*://stavox.dk/dash/bans*"
			]
		}
	]
}
