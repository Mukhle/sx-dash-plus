{
	"manifest_version": 2,

	"name": "__MSG_extName__",
	"description": "__MSG_extDesc__",
	"default_locale": "da",
	"version": "0.4.8",
	"icons": {
		"64": "icon128.png",
		"128": "icon128.png"
	},
	"permissions": [
		"*://stavox.dk/dash/*"
	],
	"content_scripts": [
		{
			"js": ["staffcheck.js"],
			"matches": ["*://stavox.dk/dash/*"]
		},
		{
			"js": ["lookup.js"],
			"matches": ["*://stavox.dk/dash/lookup*?lookupid=*"]
		},
		{
			"js": ["me.js"],
			"matches": ["*://stavox.dk/dash/me*"]
		},
		{
			"js": ["stafflist.js"],
			"matches": ["*://stavox.dk/dash/stafflist*"]
		},
		{
			"js": ["rules.js"],
			"matches": ["*://stavox.dk/dash/rules*"]
		},
		{
			"js": ["bans.js"],
			"matches": ["*://stavox.dk/dash/bans*"]
		},
		{
			"js": ["latestbans.js"],
			"matches": ["*://stavox.dk/dash/latestbans*"]
		},
		{
			"js": ["latestunbans.js"],
			"matches": ["*://stavox.dk/dash/latestunbans*"]
		},
		{
			"js": ["lookupsearch.js"],
			"matches": ["*://stavox.dk/dash/*"],
			"exclude_matches": [
				"*://stavox.dk/dash/lookup*",
				"*://stavox.dk/dash/bans*"
			]
		}
	]
}
